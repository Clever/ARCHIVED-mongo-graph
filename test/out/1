  ranksep=4;
  rankdir=LR;
  concentrate=true;
  node [shape=box, style=filled];
  graph [overlap=false];
  "1" [label="1", fillcolor="0 0.5 0.8"];
  "2" [label="2", fillcolor="0 0.5 0.8"];
  "1" -> "2" [style=solid, label="link.nested", color="0 0.5 0.8", fontcolor="0 0.5 0.8"];
  "2" -> "1" [style=solid, label="link", color="0 0.5 0.8", fontcolor="0 0.5 0.8"];
}
