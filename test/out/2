digraph {
  ranksep=4;
  rankdir=LR;
  concentrate=true;
  node [shape=box, style=filled];
  graph [overlap=false];
  "1" [label="1", fillcolor="0 0.5 0.8"];
  "1" -> "3" [style=solid, label="link", color="0 1 0.5", fontcolor="0 1 0.5"];
  "2" [label="2", fillcolor="0 0.5 0.8"];
  "2" -> "4" [style=solid, label="link", color="0 1 0.5", fontcolor="0 1 0.5"];
  "3" [label="3", fillcolor="0.3333333333333333 0.5 0.8"];
  "3" -> "1" [style=solid, label="link", color="0 1 0.5", fontcolor="0 1 0.5"];
  "4" [label="4", fillcolor="0.3333333333333333 0.5 0.8"];
  "4" -> "2" [style=solid, label="link", color="0 1 0.5", fontcolor="0 1 0.5"];
  subgraph A {
    "A" [label=A, fillColor=white];
    rank=same;
    "1";
    "2";
  }
  subgraph B {
    "B" [label=B, fillColor=white];
    rank=same;
    "3";
    "4";
  }
}
